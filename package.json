{
  "author": "Oleg Krasnov",
  "name": "react-typescript-starter-kit",
  "version": "1.0.0",
  "main": "webpack.config.ts",
  "private": "true",
  "scripts": {
    "build": "webpack --mode=production",
    "lint": "eslint --cache \"src/**/*.{ts,tsx}\" --cache-location \"./node_modules/@eslint/.eslintcache/\"",
    "prepare": "husky install",
    "prettier": "prettier src/**/*.{ts,tsx} --check",
    "start": "webpack serve --mode=development",
    "storybook": "storybook dev -p 6006",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "typescript",
    "react"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": {
    "production": [
      "last 2 years",
      "> 1%",
      "not dead"
    ],
    "development": [
      "last 2 years",
      "> 1%",
      "not dead"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/eslint-parser": "^7.24.8",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@storybook/addon-docs": "^8.2.5",
    "@storybook/addon-essentials": "^8.2.5",
    "@storybook/addon-links": "^8.2.5",
    "@storybook/addon-onboarding": "^8.2.5",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.4",
    "@storybook/blocks": "^8.2.5",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^8.2.5",
    "@storybook/react-webpack5": "^8.2.5",
    "@storybook/test": "^8.2.5",
    "@tanstack/eslint-plugin-query": "^5.51.15",
    "@types/react-lazy-load-image-component": "^1.6.4",
    "@types/react-transition-group": "^4.4.10",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "classnames": "^2.5.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.5.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.1",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.3",
    "react-transition-group": "^4.4.5",
    "sass": "^1.77.8",
    "sass-loader": "^14.2.1",
    "source-map-loader": "^5.0.0",
    "storybook": "^8.2.5",
    "stylelint": "^16.7.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-webpack-plugin": "^5.0.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "ajv": "^8.17.1",
    "postcss-media-minmax": "^5.0.0",
    "react": "^18.3.1",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1"
  }
}
